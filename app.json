{
  "expo": {
    "name": "Kotone Lite",
    "slug": "kotonelite",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "translucent": true
    },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "visible": "immersive"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.ritsu.kotone.lite"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/NunitoRoundedMplus-Regular.ttf",
            "./src/assets/fonts/NunitoRoundedMplus-Medium.ttf",
            "./src/assets/fonts/NunitoRoundedMplus-Bold.ttf"
          ]
        }
      ]
    ]
  }
}
